# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=gptFREE
OLLAMA_TEMPERATURE=0.3
OLLAMA_NUM_CTX=4096

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_TTL=3600

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost/agents_db

# API Configuration
API_HOST=0.0.0.0
API_PORT=8001
API_RELOAD=true

# Fed Job Advisor Integration
FJA_API_URL=http://localhost:8000
FJA_API_KEY=your_api_key_here

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Agent Configuration
MAX_AGENT_INSTANCES=10
AGENT_TIMEOUT_SECONDS=30
ENABLE_MERIT_COMPLIANCE=true

# Feature Flags
ENABLE_ROLE_AGENTS=true
ENABLE_COMPLIANCE_AGENTS=true
ENABLE_BACKEND_AGENTS=true